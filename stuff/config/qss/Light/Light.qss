/* -----------------------------------------------------------------------------
   Component: Tabs
----------------------------------------------------------------------------- */
.tab-container {
  background-color: #d8d8d8;
  qproperty-BottomAboveLineColor: #d8d8d8;
  qproperty-BottomBelowLineColor: #bcbcbc;
}
.tab-flat,
QTabBar::tab {
  background-color: #d8d8d8;
  border-right: 1 solid #b7b7b7;
  border-bottom: 1 solid #bcbcbc;
  color: #000;
  padding: 3 4 3 4;
}
.tab-flat:hover,
QTabBar::tab:hover {
  background-color: #e5e5e5;
  color: #000;
}
.tab-flat:selected,
QTabBar::tab:selected {
  background-color: #EFEFEF;
  color: #000;
  border-bottom-color: #EFEFEF;
}
.tab-flat:only-one,
QTabBar::tab:only-one {
  margin: 0;
}
.tab-round {
  background-color: #d8d8d8;
  border-top: 1 solid #bcbcbc;
  border-right: 1 solid #bcbcbc;
  border-left: 1 solid #bcbcbc;
  border-bottom: 1 solid #bcbcbc;
  color: #000;
  margin: 3 -1 0 0;
  padding: 2 7 1 7;
}
.tab-round:hover {
  background-color: #e5e5e5;
  color: #000;
}
.tab-round:selected {
  background-color: #EFEFEF;
  border-top-right-radius: 2;
  border-top-left-radius: 2;
  border-bottom-color: #EFEFEF;
  color: #000;
  margin: 1 -1 -1 0;
  padding: 2 7 2 7;
}
.tab-round:only-one {
  margin: 1 0 0 0;
  padding: 3 7 3 7;
}
.tab-round:last {
  margin-right: 0;
  border-top-right-radius: 2;
}
.tab-round:first {
  border-top-left-radius: 2;
}
