/* -----------------------------------------------------------------------------
   Component: Tabs
----------------------------------------------------------------------------- */
.tab-container {
  background-color: #393939;
  qproperty-BottomAboveLineColor: #393939;
  qproperty-BottomBelowLineColor: #2c2c2c;
}
.tab-flat,
QTabBar::tab {
  background-color: #393939;
  border-right: 1 solid #272727;
  border-bottom: 1 solid #2c2c2c;
  color: #a1a1a1;
  padding: 3 4 3 4;
}
