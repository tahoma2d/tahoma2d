  background-color: #454545;
  color: #a1a1a1;
}
.tab-round:selected {
  background-color: #484848;
  border-top-right-radius: 2;
  border-top-left-radius: 2;
  border-bottom-color: #484848;
  color: #fff;
  margin: 1 -1 -1 0;
  padding: 2 7 2 7;
}
.tab-round:only-one {
  margin: 1 0 0 0;
  padding: 3 7 3 7;
}
.tab-round:last {
  margin-right: 0;
  border-top-right-radius: 2;
}
.tab-round:first {
  border-top-left-radius: 2;
}
